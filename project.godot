; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Eryndor 4.0 Final"
config/version="1.0.0"
run/main_scene="res://scenes/MainMenu.tscn"
config/use_hidden_project_data_directory=false
config/features=PackedStringArray("4.5", "Forward Plus")
run/max_fps=65
run/low_processor_mode=true

[autoload]

Eryndor="*res://core/singletons/eryndor.gd"
MythosLogger="*res://core/singletons/Logger.gd"
WorldStreamer="*res://core/streaming/world_streamer.gd"
EntitySim="*res://core/sim/entity_sim.gd"
FactionEconomy="*res://core/sim/faction_economy.gd"
PerformanceMonitorSingleton="*res://core/singletons/PerformanceMonitorSingleton.gd"
PerformanceLogger="*res://core/singletons/PerformanceLogger.gd"
FlameGraphProfiler="*res://core/singletons/FlameGraphProfiler.gd"
AzgaarIntegrator="*res://scripts/managers/AzgaarIntegrator.gd"
AzgaarServer="*res://scripts/managers/AzgaarServer.gd"
LoadingOverlay="*res://core/singletons/LoadingOverlay.gd"

[debug]

settings/profiler/max_functions=1000
settings/show_fps=false
settings/show_memory=false
settings/show_collision_shapes=false
settings/show_navigation=false
settings/show_paths=false
settings/show_visibility_rects=false
settings/profiler/enabled=true

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/size/initial_size=1280
x720window/size/windowed_scale=0.75

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg", "res://addons/procedural_world_map/plugin.cfg")

[engine]

godot_version="4.3.stable"

